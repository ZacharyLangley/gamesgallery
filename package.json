{
  "name": "gamegallery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:prod": "NODE_ENV=production react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:prod": "NODE_ENV=production react-native run-ios --configuration Release",
    "start": "react-native start",
    "start:prod": "NODE_ENV=production react-native start",
    "build:android": "cd android && ./gradlew assembleRelease",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "eslint .",
    "test": "jest",
    "packages": "npm install --legacy-peer-deps && cd ios && pod install && cd ..",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" && prettier --check \"shared/**/*.{js,jsx,ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-vector-icons/ant-design": "^4.4.2",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/evil-icons": "^1.10.1",
    "@react-native-vector-icons/feather": "^12.0.0",
    "@react-native-vector-icons/fontawesome6": "^6.7.1",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.3.13",
    "@react-navigation/stack": "^7.1.2",
    "i18next": "^25.2.0",
    "react": "19.0.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.3",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.1",
    "react-native-svg": "^15.12.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@types/jest": "^29.5.13",
    "@types/prettier": "^2.7.3",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}